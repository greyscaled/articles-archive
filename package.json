{
  "name": "vapurrmaid-articles-archive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "inspect": "vue inspect > inspect-out.js",
    "test": "npm run lint"
